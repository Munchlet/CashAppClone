{
	"compilerOptions": {
		"module": "es6",
		"target": "es6",
		"allowSyntheticDefaultImports": true,
		"jsx": "react-native",
		"lib": ["dom", "esnext"],
		"moduleResolution": "node",
		"noEmit": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"strict": true,
		"types": [
			"react",
			"react-native",
			"jest"
		]
	}
}
